{
    ":services": [
        {
            ":name": "ElastiCache Clusters",
            ":client": ":elasticache",
            ":describe_method": "describe_cache_clusters",
            ":collection": "cache_clusters",
            ":attributes": [
                {
                    ":column_name": "ID",
                    ":type": ":scalar",
                    ":target": "cache_cluster_id",
                    ":column_width": 20,
                    ":count": true
                },
                {
                    ":column_name": "Type",
                    ":type": ":scalar",
                    ":target": "cache_node_type",
                    ":column_width": 10
                },
                {
                    ":column_name": "Engine",
                    ":type": ":scalar",
                    ":target": "engine",
                    ":column_width": 10
                },
                {
                    ":column_name": "Version",
                    ":type": ":scalar",
                    ":target": "engine_version",
                    ":column_width": 10
                },
                {
                    ":column_name": "Status",
                    ":type": ":scalar",
                    ":target": "cache_cluster_status",
                    ":column_width": 15
                },
                {
                    ":column_name": "Node count",
                    ":type": ":scalar",
                    ":target": "num_cache_nodes",
                    ":column_width": 10,
                    ":count": true
                },
                {
                    ":column_name": "Replication Group",
                    ":type": ":scalar",
                    ":target": "replication_group_id",
                    ":column_width": 20
                },
                {
                    ":column_name": "Node ID",
                    ":type": ":list",
                    ":collection": "cache_nodes",
                    ":target": "cache_node_id",
                    ":column_width": 10
                },
                {
                    ":column_name": "Node Status",
                    ":type": ":list",
                    ":collection": "cache_nodes",
                    ":target": "cache_node_status",
                    ":column_width": 10
                },
                {
                    ":column_name": "Created",
                    ":type": ":list",
                    ":collection": "cache_nodes",
                    ":target": "cache_node_create_time",
                    ":column_width": 15
                },
                {
                    ":column_name": "Source node",
                    ":type": ":list",
                    ":collection": "cache_nodes",
                    ":target": "source_cache_node_id",
                    ":column_width": 15
                }
            ]
        },
        {
            ":name": "ElastiCache Replication Groups",
            ":client": ":elasticache",
            ":describe_method": "describe_replication_groups",
            ":collection": "replication_groups",
            ":attributes": [
                {
                    ":type": ":scalar",
                    ":column_name": "ID",
                    ":target": "replication_group_id",
                    ":column_width": 20,
                    ":count": true
                },
                {
                    ":type": ":scalar",
                    ":column_name": "Status",
                    ":target": "status",
                    ":column_width": 15
                },
                {
                    ":type": ":scalar",
                    ":column_name": "Description",
                    ":target": "description",
                    ":column_width": 15
                },
                {
                    ":type": ":scalar",
                    ":column_name": "Automatic Failover",
                    ":target": "automatic_failover",
                    ":column_width": 15
                },
                {
                    ":type": ":array",
                    ":name": "Node Groups",
                    ":collection": "node_groups",
                    ":attributes": [
                      {
                          ":type": ":scalar",
                          ":column_name": "ID",
                          ":target": "node_group_id",
                          ":column_width": 15
                      },
                      {
                          ":type": ":scalar",
                          ":column_name": "Status",
                          ":target": "status",
                          ":column_width": 15
                      },
                      {
                          ":type": ":array",
                          ":name": "Node Group Members",
                          ":collection": "node_group_members",
                          ":attributes": [
                            {
                                ":type": ":scalar",
                                ":column_name": "Cluster ID",
                                ":target": "cache_cluster_id",
                                ":column_width": 15
                            },
                            {
                                ":type": ":scalar",
                                ":column_name": "Node ID",
                                ":target": "cache_node_id",
                                ":column_width": 15
                            },
                            {
                                ":type": ":scalar",
                                ":column_name": "Current Role",
                                ":target": "current_role",
                                ":column_width": 15
                            }
                          ]
                      }
                    ]
                }
            ]
        }
    ]
}
